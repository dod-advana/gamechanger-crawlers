    "configurations": [
        {
            "name": "DoDStrategy",
            "type": "python",
            "request": "launch",
            "module": "scrapy",
            "args": [
                "runspider",
                "dataPipelines/gc_scrapy/gc_scrapy/spiders/dod_strategy_spider.py",
                "-a",
                "download_output_dir=tmp/dod_strategy_spider",
